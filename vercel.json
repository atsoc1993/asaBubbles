{
    "version": 2,
    "builds": [
      {
        "src": "api/bubbles.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/bubbles.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  